{
    "manifest_version" : 3,
    "name" : "autoCheck",
    "description" : "auto check input type checkbox extention.",
    "version" : "1.0",
    "icons" : {
        "16" : "/images/checkbox-48.png",
        "48" : "/images/checkbox-48.png",
        "128" : "/images/checkbox-48.png"
    },
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["main.js"]
        }
    ]
}